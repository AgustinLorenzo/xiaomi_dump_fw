config config repacd
	option Enable                           '0'
	option ForceWextMode                    '0'
	option ManagedNetwork                   'lan'
	option DeviceType                       'RE'    # this option no sense both in CAP and RE
	option Role                             'NonCAP'
	option GatewayConnectedMode             'AP'    # only valid when CAP in full-bridge mode
	option ConfigREMode                     'son'
	option DefaultREMode                    'son'
	option BlockDFSChannels                 '0'
	option EnableSteering                   '1'
	option EnableSON                        '1'
	option ManageMCSD                       '1'
	option LinkCheckDelay                   '2'
	# asure that we do NOT turn on guest TS
	option TrafficSeparationEnabled         '0'
	option TrafficSeparationActive          '0'
	option NetworkGuest                     'guest'
	option NetworkGuestBackhaulInterface    'both'
	option EnableEthernetMonitoring         '0'
	# disable auto set GWMON_MODE_CAP when detect router in gwmon.sh
	option EnableMixedBackhaul              '1'
	# miwifi: we need CAP must be router-mode, thus app/web UI must assure that wan is configed before repacd init as CAP.
	# however whc make sure that CAP-init will always init as a CAP
	option miwifi_mode                      'none'    # miwifi add: force as cap / re

