	option 5GBackhaulBadlinkTimeout         '60'
	option BSSIDAssociationTimeout          '90'
	option RateScalingFactor                '70'
	option 5GBackhaulEvalTimeShort          '330' # default 1800
	option 5GBackhaulEvalTimeLong           '1800' # default 7200
	option 2GBackhaulEvalTime               '1800' # default 1800
	option PreferCAPSNRThreshold5G          '20'
	option MoveFromCAPSNRHysteresis5G       '10'
